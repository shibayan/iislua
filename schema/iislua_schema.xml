      <attribute name="connectTimeout" type="timeSpan" defaultValue="00:01:00" validationType="timeSpanRange" validationParameter="0,65535,1" />
      <attribute name="sendTimeout" type="timeSpan" defaultValue="00:01:00" validationType="timeSpanRange" validationParameter="0,65535,1" />
      <attribute name="readTimeout" type="timeSpan" defaultValue="00:01:00" validationType="timeSpanRange" validationParameter="0,65535,1" />
    </element>
    <element name="beginRequest">
      <attribute name="scriptPath" type="string" />
    </element>
    <element name="authenticateRequest">
      <attribute name="scriptPath" type="string" />
    </element>
    <element name="authorizeRequest">
      <attribute name="scriptPath" type="string" />
    </element>
    <element name="executeRequest">
      <attribute name="scriptPath" type="string" />
    </element>
    <element name="logRequest">
      <attribute name="scriptPath" type="string" />
    </element>
    <element name="endRequest">
      <attribute name="scriptPath" type="string" />
    </element>
    <element name="mapPath">
      <attribute name="scriptPath" type="string" />
    </element>
  </sectionSchema>
</configSchema>